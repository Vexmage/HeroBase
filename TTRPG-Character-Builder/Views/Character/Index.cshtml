@model List<TTRPG_Character_Builder.Models.Character>

@{
    ViewData["Title"] = "Character List";
}

<h1>Character List</h1>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Description</th>
           
        </tr>
    </thead>
    <tbody>
        @foreach (var character in Model)
        {
            <tr>
                <td>@character.Name</td>
                <td>@character.Biography</td>
                
            </tr>
        }
    </tbody>
</table>
